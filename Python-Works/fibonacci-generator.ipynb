    "# Generator ile fibonacci serisi\n",
    "def fibonacci():\n",
    "    a = 1\n",
    "    b = 1\n",
    "    yield a, # generator ile Ã¼retelim\n",
    "    yield b\n",
    "\n",
    "    while True:\n",
    "        a, b = b, a + b\n",
    "        yield b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for sayi in fibonacci():\n",
    "    if sayi > 100000:\n",
    "        break\n",
    "    print(sayi)"
   ]
  }
