   "source": [
    "while True:\n",
    "    sayi = input(\"Sayı : \")\n",
    "    if sayi == \"q\":\n",
    "        print(\"Program tamamlandı...\")\n",
    "        break\n",
    "    else:\n",
    "        sayi = int(sayi)\n",
    "        faktoriyel = 1\n",
    "        for i in range(2, sayi + 1):\n",
    "            faktoriyel *= i\n",
    "        print(\"Faktöriyel : \", faktoriyel)"
   ]
  }
